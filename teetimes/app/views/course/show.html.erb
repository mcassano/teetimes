<h1><%= @course.name %></h1>
Date: <%= seven_days_from_today %>
<ul>
<% @output.each do |opening| %>
  <li>
    Course: <%= opening['name'] %><br/>
    Time: <%= minutes_since_midnight_to_time(opening['teeTime']) %><br/>
    Number of slots open: <%= 4 - opening['playerCount'] %><br/>
  </li>
<% end %>
</ul>